CREATE TABLE Plant (
	id	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
	main_common_name	TEXT NOT NULL,
	scientific_name	TEXT NOT NULL UNIQUE,
	family	TEXT NOT NULL,
	image_url	TEXT NOT NULL
);

getPlant:
SELECT * FROM Plant WHERE scientific_name == ?;

getAll:
SELECT * FROM Plant;

insert:
INSERT INTO Plant(scientific_name, main_common_name, family, image_url) VALUES (?, ?, ?, ?);

deleteAll:
DELETE FROM Plant;