import com.cramsan.petproject.appcore.model.AnimalType;

CREATE TABLE Animal (
	id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
	common_name	TEXT AS AnimalType NOT NULL
);

getAnimal:
SELECT * FROM Animal WHERE common_name == ?;

getAll:
SELECT * FROM Animal;

insert:
INSERT INTO Animal(common_name) VALUES (?);

clear:
DELETE FROM Animal;