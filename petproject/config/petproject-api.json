        "/description/{plantId}/{animalId}": {
            "get": {
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/Description"
                    }
                }
            },
            "parameters": [
                {
                    "name": "plantId",
                    "schema": {
                        "type": "string"
                    },
                    "in": "path",
                    "required": true
                },
                {
                    "name": "animalId",
                    "schema": {
                        "type": "string"
                    },
                    "in": "path",
                    "required": true
                }
            ]
        },
