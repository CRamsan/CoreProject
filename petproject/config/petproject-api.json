                        "type": "string"
                    },
                    "family": {
                        "type": "string"
                    },
                    "plantId": {
                        "format": "int32",
                        "type": "integer"
                    }
                },
                "example": {
                    "id": 199,
                    "locale": "en",
                    "family": "Oleaceae",
                    "plantId": 199
                }
            },
            "CommonName": {
                "title": "Root Type for CommonName",
                "description": "",
                "type": "object",
                "properties": {
                    "id": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "plantId": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "locale": {
                        "type": "string"
                    },
                    "commonName": {
                        "type": "string"
                    }
                },
                "example": {
                    "id": 574,
                    "plantId": 200,
                    "locale": "en",
                    "commonName": "White Heads"
                }
            },
            "MainName": {
                "title": "Root Type for MainName",
