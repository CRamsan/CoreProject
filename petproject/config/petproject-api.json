            "parameters": [
                {
                    "name": "plantId",
                    "schema": {
                        "type": "string"
                    },
                    "in": "path",
                    "required": true
                }
            ]
        },
        "/toxicity/{plantId}": {
            "get": {
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/Toxicity"
                    }
                }
            },
            "parameters": [
                {
                    "name": "plantId",
                    "schema": {
                        "type": "string"
                    },
                    "in": "path",
                    "required": true
                }
            ]
        }
    },
    "components": {
        "schemas": {
            "Toxicity": {
                "title": "Root Type for Toxicity",
                "description": "Toxicity value",
                "type": "object",
                "properties": {
                    "id": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "plantId": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "animalId": {
                        "type": "string"
                    },
                    "source": {
                        "type": "string"
                    },
                    "toxic": {
                        "type": "string"
                    }
                },
                "example": {
                    "id": 1,
                    "plantId": 1,
                    "animalId": "DOG",
                    "source": "https://www.aspca.org/pet-care/animal-poison-control/toxic-and-non-toxic-plants/forster-sentry-palm",
                    "toxic": "NON_TOXIC"
                }
            },
            "Plant": {
                "title": "Root Type for Plant",
                "description": "A single plant entity",
                "type": "object",
                "properties": {
                    "id": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "imageUrl": {
                        "type": "string"
                    },
                    "scientificName": {
                        "type": "string"
                    }
                },
                "example": {
                    "id": 1,
                    "imageUrl": "https://upload.wikimedia.org/wikipedia/commons/thumb/9/99/Howea_forsteriana_Lord_Howe_Island.jpg/1200px-Howea_forsteriana_Lord_Howe_Island.jpg",
                    "scientificName": "Howea forsteriana"
                }
            },
            "Description": {
                "title": "Root Type for Description",
                "description": "",
                "type": "object",
                "properties": {
                    "id": {
                        "format": "int32",
                        "type": "integer"
                    },
                    "locale": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "animalId": {
                        "type": "string"
                    },
                    "plantId": {
                        "format": "int32",
                        "type": "integer"
                    }
                },
                "example": {
                    "id": 1,
                    "locale": "en",
                    "description": "",
                    "animalId": "DOG",
                    "plantId": 1
                }
            },
            "Family": {
                "title": "Root Type for Family",
                "description": "",
                "type": "object",
                "properties": {
                    "id": {
                        "format": "int32",
                        "type": "integer"
                    },
