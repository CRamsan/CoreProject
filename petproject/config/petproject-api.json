                        "format": "int32",
                        "type": "integer"
                    },
                    "locale": {
                        "type": "string"
                    },
                    "mainName": {
                        "type": "string"
                    }
                },
                "example": {
                    "id": 1,
                    "plantId": 1,
                    "locale": "en",
                    "mainName": "Forster Sentry Palm"
                }
            }
