{
    "openapi": "3.0.1",
    "info": {
        "title": "petproject-api",
        "version": "1.0",
        "description": "PetProject API",
        "contact": {

        }
    },
    "servers": [
        {
            "url": "https://{domain}-petproject.azure-api.net/petproject-api",
            "description": "",
            "variables": {
                "domain": {
                    "default": "prod"
                }
            }
        }
    ],
    "paths": {
        "/description": {
            "get": {
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/DescriptionList"
                    }
                }
            }
        },
        "/family": {
            "get": {
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/FamilyList"
                    }
                }
            }
        },
        "/plant": {
            "get": {
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/PlantList"
                    }
                }
            }
        },
        "/toxicity": {
            "get": {
